{
    "requestApproval": {
        "schema": {
            "method": "POST",
            "host": "<%= iparam.domain %>",
            "path": "/api/v2/tickets/<%= context.ticket_id %>/approvals",
            "headers": {
                "Authorization": "Basic <%= encode(iparam.apikey) %>",
                "Content-Type": "application/json"
            }
        },
        "options": {}
    },
