{
    "getRequestedItems": {
        "schema": {
            "method": "GET",
            "host": "<%= iparam.domain %>",
            "path": "/api/v2/tickets/<%= context.id %>/requested_items",
            "headers": {
                "Authorization": "Basic <%= encode(iparam.apikey) %>",
                "Content-Type": "application/json"
            }
        },
        "options": {}
    },
    "getDetailTicket": {
        "schema": {
            "method": "GET",
            "host": "<%= iparam.domain %>",
            "path": "/api/v2/objects/27000052163/records?query=sr_id%20%3A%20%27<%= context.id %>%27%20&sort_field=leave_date&sord_order=ASC",
            "headers": {
                "Authorization": "Basic <%= encode(iparam.apikey) %>",
                "Content-Type": "application/json"
            }
        },
        "options": {}
    }
}